        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the type of a managed code policy level.</source>
          <target state="translated">Określa typ poziomu zasad kodu zarządzanego.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The highest level of security policy is enterprise-wide.</source>
          <target state="translated">Najwyższy poziom zabezpieczeń zasad jest całego przedsiębiorstwa.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
