        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Returns the name of the task.</source>
          <target state="translated">返回任务的名称。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Gets the type information for all task parameters.</source>
          <target state="translated">获取所有任务参数的类型信息。</target>       </trans-unit>
