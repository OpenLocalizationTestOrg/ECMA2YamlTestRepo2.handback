      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the permission level for audio.</source>
          <target state="translated">指定音频权限级别。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Use this enumeration to set the MediaPermissionAudio property of the &lt;xref:System.Security.Permissions.MediaPermission&gt; class.</source>
          <target state="translated">使用此枚举设置 MediaPermissionAudio 属性的&lt;xref:System.Security.Permissions.MediaPermission&gt;类。&lt;/xref:System.Security.Permissions.MediaPermission&gt;</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The default is SafeAudio.</source>
          <target state="translated">默认值为 SafeAudio。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Permission that allows unrestricted audio playback.</source>
          <target state="translated">允许不受限制的音频播放的权限。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">要添加。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Permission that denies all audio playback.</source>
          <target state="translated">拒绝所有音频播放的权限。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">要添加。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Permission that allows safe, limited audio playback, which includes denying stream access to the image file.</source>
          <target state="translated">允许对安全的权限有限音频播放，包括拒绝对图像文件流访问。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">要添加。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Permission that only allows audio playback of files that are located at the application's site of origin.</source>
          <target state="translated">只允许音频播放的位于应用程序的源站点的文件的权限。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">要添加。</target>       </trans-unit>
