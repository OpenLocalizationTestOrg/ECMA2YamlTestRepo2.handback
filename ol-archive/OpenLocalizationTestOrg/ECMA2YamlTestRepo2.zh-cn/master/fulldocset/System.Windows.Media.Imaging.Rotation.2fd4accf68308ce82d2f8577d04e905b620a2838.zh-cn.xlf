        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the rotation to apply to a bitmap image.</source>
          <target state="translated">指定要应用到位图图像的旋转。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Only 90 degree increments are supported.</source>
          <target state="translated">支持唯一 90 度的增量。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The bitmap is not rotated.</source>
          <target state="translated">不旋转位图。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>This is the default value.</source>
          <target state="translated">此为默认值。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">要添加。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Rotate the bitmap clockwise by 180 degrees.</source>
          <target state="translated">将位图顺时针旋转 180 度。</target>       </trans-unit>
