        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>An XML element that defines the security settings for a wsHttpBinding binding.</source>
          <target state="translated">Az XML-elem, amely meghatározza a wsHttpBinding kötés biztonsági beállításait.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.ServiceModel.Configuration.WSHttpSecurityElement"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class.</source>
          <target state="translated">Egy új példányát inicializálja a <bpt id="p1">&lt;xref href="System.ServiceModel.Configuration.WSHttpSecurityElement"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> osztály.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the security settings for the message.</source>
          <target state="translated">Lekérdezi az üzenet biztonsági beállításait.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>A <bpt id="p1">&lt;xref href="System.ServiceModel.Configuration.MessageSecurityOverHttpElement"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> that specifies the security settings for the message.</source>
