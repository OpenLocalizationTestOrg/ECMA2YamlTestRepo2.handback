      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>A task that returns values as specified by XPath Query from an XML file.</source>
          <target state="translated">XML ファイルからの XPath クエリで指定された値を表すタスク。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Creates a task that returns values as specified by XPath Query from an XML file.</source>
          <target state="translated">XML ファイルからの XPath クエリで指定された値を返すタスクを作成します。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Executes the XMLPeek task.</source>
          <target state="translated">XMLPeek タスクを実行します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Returns true if transformation succeeds; false otherwise.</source>
          <target state="translated">変換が成功した場合、true を返しますfalse それ以外の場合。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Gets or sets the namespaces for XPath query's prefixes.</source>
          <target state="translated">取得または XPath クエリのプレフィックスの名前空間を設定します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Returns the namespaces.</source>
          <target state="translated">名前空間を返します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Gets or sets the XPath Query.</source>
          <target state="translated">取得または XPath クエリを設定します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Returns the XPath Query.</source>
          <target state="translated">XPath クエリを返します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Gets the results returned by this task.</source>
          <target state="translated">このタスクによって返される結果を取得します。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Returns the results.</source>
          <target state="translated">結果を返します。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Gets or sets the XML input as a string.</source>
          <target state="translated">取得または文字列としての XML 入力を設定します。</target>       </trans-unit>
