      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Indicates the formatter that is used to serialize objects that are sent as the body of an MSMQ message.</source>
          <target state="translated">MSMQ メッセージの本文として送信されるオブジェクトのシリアル化に使用されるフォーマッタを示します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>The default value is Xml.</source>
          <target state="translated">既定値は Xml です。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The ActiveX formatter is used to serialize the object.</source>
          <target state="translated">ActiveX フォーマッタは、オブジェクトをシリアル化に使用されます。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">追加します。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">&lt;xref href="System.Runtime.Serialization.Formatters.Binary.BinaryFormatter"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> is used to serialize the object.</source>
          <target state="translated"><bpt id="p1">&lt;xref href="System.Runtime.Serialization.Formatters.Binary.BinaryFormatter"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept>オブジェクトをシリアル化するために使用します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">追加します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Serializes the object to an array of bytes.</source>
          <target state="translated">バイトの配列にオブジェクトをシリアル化します。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">追加します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Serializes the object to a stream.</source>
          <target state="translated">ストリームにオブジェクトをシリアル化します。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>To be added.</source>
          <target state="translated">追加します。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>&lt;xref:System.Xml.Serialization.XmlSerializer&gt; is used to serialize the object.</source>
          <target state="translated">&lt;xref:System.Xml.Serialization.XmlSerializer&gt;オブジェクトをシリアル化に使用されます。&lt;/xref:System.Xml.Serialization.XmlSerializer&gt;</target>       </trans-unit>
