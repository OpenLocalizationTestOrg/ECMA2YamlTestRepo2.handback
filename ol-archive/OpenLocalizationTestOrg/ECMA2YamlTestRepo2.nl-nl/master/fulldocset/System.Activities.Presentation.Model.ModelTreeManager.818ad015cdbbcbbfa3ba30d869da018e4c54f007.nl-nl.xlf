        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>The model item for the manager.</source>
          <target state="translated">De modelitem voor de manager.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Loads the specified root instance.</source>
          <target state="translated">De opgegeven hoofdexemplaar laadt.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>The root instance to load.</source>
          <target state="translated">Het basis-exemplaar te laden.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>Gets the root.</source>
          <target state="translated">Hiermee haalt u de hoofdmap.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">&lt;xref href="System.Activities.Presentation.Model.ModelItem"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> object that contains the root.</source>
