        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Gets the new state of the connection.</source>
          <target state="translated">Hiermee haalt u de nieuwe status van de verbinding.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
