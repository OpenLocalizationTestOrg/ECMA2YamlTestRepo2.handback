        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>A &lt;xref:System.ServiceModel.Channels.Binding&gt; object that represents the binding to be added.</source>
          <target state="translated">A&lt;xref:System.ServiceModel.Channels.Binding&gt;对象表示要添加的绑定。&lt;/xref:System.ServiceModel.Channels.Binding&gt;</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
