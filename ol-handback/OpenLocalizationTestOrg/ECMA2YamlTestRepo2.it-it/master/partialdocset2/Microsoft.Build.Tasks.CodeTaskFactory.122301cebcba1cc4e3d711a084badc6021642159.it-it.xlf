          <target state="translated">I parametri per l'attività.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>The element contents for the task.</source>
          <target state="translated">Contenuto dell'elemento per l'attività.</target>       </trans-unit>
