        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>A task factory which can take a code DOM supported language and create a task from it.</source>
          <target state="translated">Factory delle attività che può richiedere un codice DOM lingua supportata e utilizzarlo per creare un'attività.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Creates the task factory.</source>
          <target state="translated">Crea la factory delle attività.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Cleans up any context or state used in a task.</source>
          <target state="translated">Pulisce qualsiasi contesto o stato utilizzato in un'attività.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The task to clean up.</source>
          <target state="translated">L'attività di pulizia.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Create a <ph id="ph1">`taskfactory`</ph> instance which contains the data that needs to be refreshed between task invocations.</source>
          <target state="translated">Creare un <ph id="ph1">`taskfactory`</ph> istanza che contiene i dati che devono essere aggiornate tra le chiamate all'attività.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The host that logs information from the task factory.</source>
          <target state="translated">L'host che registra informazioni dalla factory delle attività.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Returns the new task.</source>
          <target state="translated">Restituisce la nuova attività.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The MSBuild engine uses this for logging.</source>
          <target state="translated">Il motore MSBuild utilizza per la registrazione.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Returns the name of the task.</source>
          <target state="translated">Restituisce il nome dell'attività.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Gets the type information for all task parameters.</source>
          <target state="translated">Ottiene le informazioni sul tipo per tutti i parametri di attività.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Returns an array of the type information for all task parameters.</source>
          <target state="translated">Restituisce una matrice delle informazioni sul tipo per tutti i parametri di attività.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
