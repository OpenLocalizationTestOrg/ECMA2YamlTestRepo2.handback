      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents a reference to a field.</source>
          <target state="translated">Представляет ссылку на поле.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>CodeFieldReferenceExpression can be used to represent a reference to a field.</source>
          <target state="translated">CodeFieldReferenceExpression может использоваться для представления ссылка на поле.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The &lt;xref:System.CodeDom.CodeFieldReferenceExpression.TargetObject%2A&gt; property specifies the object that contains the field.</source>
          <target state="translated">&lt;xref:System.CodeDom.CodeFieldReferenceExpression.TargetObject%2A&gt;Свойство указывает объект, содержащий поле.&lt;/xref:System.CodeDom.CodeFieldReferenceExpression.TargetObject%2A&gt;</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>The &lt;xref:System.CodeDom.CodeFieldReferenceExpression.FieldName%2A&gt; property specifies the name of the field to reference.</source>
          <target state="translated">&lt;xref:System.CodeDom.CodeFieldReferenceExpression.FieldName%2A&gt;Свойство задает имя поля для ссылки.&lt;/xref:System.CodeDom.CodeFieldReferenceExpression.FieldName%2A&gt;</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.CodeDom.CodeFieldReferenceExpression"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class.</source>
          <target state="translated">Инициализирует новый экземпляр <bpt id="p1">&lt;xref href="System.CodeDom.CodeFieldReferenceExpression"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> класса.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.CodeDom.CodeFieldReferenceExpression"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class using the specified target object and field name.</source>
          <target state="translated">Инициализирует новый экземпляр <bpt id="p1">&lt;xref href="System.CodeDom.CodeFieldReferenceExpression"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> класса с помощью указанного целевого объекта и имя поля.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>A <bpt id="p1">&lt;xref href="System.CodeDom.CodeExpression"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> that indicates the object that contains the field.</source>
          <target state="translated">Объект <bpt id="p1">&lt;xref href="System.CodeDom.CodeExpression"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> указывает объект, содержащий поле.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The name of the field.</source>
          <target state="translated">Имя поля.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Gets or sets the name of the field to reference.</source>
          <target state="translated">Возвращает или задает имя поля для ссылки.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>A string containing the field name.</source>
          <target state="translated">Строка, содержащая имя поля.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Gets or sets the object that contains the field to reference.</source>
          <target state="translated">Возвращает или задает объект, содержащий поле для ссылки.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>A <bpt id="p1">&lt;xref href="System.CodeDom.CodeExpression"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> that indicates the object that contains the field to reference.</source>
