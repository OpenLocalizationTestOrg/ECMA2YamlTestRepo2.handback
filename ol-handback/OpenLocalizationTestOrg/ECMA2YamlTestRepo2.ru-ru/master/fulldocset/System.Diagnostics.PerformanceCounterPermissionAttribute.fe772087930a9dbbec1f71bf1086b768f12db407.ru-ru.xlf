        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>The default is <bpt id="p1">&lt;xref href="System.Diagnostics.EventLogPermissionAccess"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept>.</source>
          <target state="translated">Значение по умолчанию — <bpt id="p1">&lt;xref href="System.Diagnostics.EventLogPermissionAccess"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept>.</target>       </trans-unit>
