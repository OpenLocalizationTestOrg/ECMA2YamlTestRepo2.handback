        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.Net.Sockets.IPv6MulticastOption"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class with the specified IP multicast group and the local interface address.</source>
          <target state="translated">Egy új példányát inicializálja a <bpt id="p1">&lt;xref href="System.Net.Sockets.IPv6MulticastOption"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> osztály a megadott IP csoportos és a helyi kapcsolat címen.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">`ifindex`</ph> parameter specifies the interface on which data is received or sent.</source>
          <target state="translated">A <ph id="ph1">`ifindex`</ph> paraméter határozza meg a felület, amelyen adatok fogadott vagy elküldött.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The group &lt;xref:System.Net.IPAddress&gt;.</source>
          <target state="translated">A csoport &lt;xref:System.Net.IPAddress&gt;.&lt;/xref:System.Net.IPAddress&gt;</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The local interface address.</source>
          <target state="translated">A helyi kapcsolat címe.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source><ph id="ph1">&lt;code&gt;ifindex&lt;/code&gt;</ph> is less than 0.</source>
          <target state="translated"><ph id="ph1">&lt;code&gt;ifindex&lt;/code&gt;</ph>értéke 0-nál kisebb.</target>       </trans-unit>
