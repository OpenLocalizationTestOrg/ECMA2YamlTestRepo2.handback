        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Defines the contract between <ph id="ph1">[!INCLUDE[vs2010](~/add/includes/vs2010-md.md)]</ph> and the <ph id="ph2">[!INCLUDE[wfd1](~/add/includes/wfd1-md.md)]</ph> for integrated help provided.</source>
          <target state="translated">Határozza meg a szerződés között <ph id="ph1">[!INCLUDE[vs2010](~/add/includes/vs2010-md.md)]</ph> és a <ph id="ph2">[!INCLUDE[wfd1](~/add/includes/wfd1-md.md)]</ph> megadott integrált segítségét.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Adds the context attribute.</source>
          <target state="translated">A context attribútum hozzáadása.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The name of the context attribute to add.</source>
          <target state="translated">A környezet attribútum hozzáadása neve.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The value of the context attribute to add.</source>
          <target state="translated">A hozzáadandó környezetben attribútum értéke.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">&lt;xref href="System.ComponentModel.Design.HelpKeywordType"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> that contains identifiers that indicate the type of a Help keyword.</source>
          <target state="translated">A <bpt id="p1">&lt;xref href="System.ComponentModel.Design.HelpKeywordType"&gt;</bpt> <ept id="p1">&lt;/xref&gt;</ept> tartalmazó azonosítók súgó kulcsszó típusát jelzi.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Removes the context attribute.</source>
          <target state="translated">Eltávolítja a context attribútum.</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The name of the context attribute to remove.</source>
          <target state="translated">Eltávolítja a környezet attribútum neve.</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The value of the context attribute to remove.</source>
          <target state="translated">Eltávolítja a környezet attribútum értéke.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Shows the Help topic that corresponds to the specified keyword.</source>
          <target state="translated">A Súgó-témakör, amely megfelel a megadott kulcsszó jeleníti meg.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The keyword of the Help topic to display.</source>
          <target state="translated">A kulcsszó a témakör megjelenítése.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Shows the Help topic that corresponds to the specified URL.</source>
          <target state="translated">Jelenít meg a Súgó-témakör, amely megfelel a megadott URL-cím.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>The URL of the Help topic to display.</source>
          <target state="translated">A megjelenítendő URL-című súgótémakört.</target>       </trans-unit>
