        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the border styles for a form.</source>
          <target state="translated">Określa styl obramowania formularza.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>This enumeration is used by the &lt;xref:System.Windows.Forms.Form&gt; class.</source>
          <target state="translated">To wyliczenie jest używane przez &lt;xref:System.Windows.Forms.Form&gt;klasę.&lt;/xref:System.Windows.Forms.Form&gt;</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>It represents the different styles of the form.</source>
          <target state="translated">Reprezentuje różne style formularza.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>The default style is <ph id="ph1">`Sizable`</ph>.</source>
          <target state="translated">Domyślny styl jest <ph id="ph1">`Sizable`</ph>.</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>A fixed, three-dimensional border.</source>
          <target state="translated">Stały, trójwymiarowe obramowanie.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
